@model IEnumerable<CategoryModel>

<h1>Category</h1>

<table class="table">
    <thead></thead>
    <tr>
    <th>Category Name</th>
    <th>Display Order</th>
    </tr>
    @foreach (var item in Model)
{
    <tbody>
    <tr>
    <td>@item.Name</td>
    <td>@item.Order</td>
    </tr>
    </tbody>
}
</table>

